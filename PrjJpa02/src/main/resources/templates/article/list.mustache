{{>layout/header}}
 
<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">제목</th>
      <th scope="col">내용</th>
    </tr>
  </thead>
  <tbody>
  	{{#articleList}}
    <tr>
      <th scope="row">{{id}}</th>
      <td><a href="/article/{{id}}">{{title}}</a></td>
      <td>{{content}}</td>
    </tr>
		{{/articleList}}
  </tbody>
</table>
<p>{{totCount}}</p>  
<a href="/article/WriteForm" class="btn btn-primary">새글 쓰기</a>
      
{{>layout/footer}}